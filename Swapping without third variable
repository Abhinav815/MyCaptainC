#include<stdio.h>  
 int main()    
{    
    int a=5, b=10; //2 numbers taken     
    printf("Before swap a=%d b=%d",a,b);      
    a=a+b;    
    b=a-b; //now b stores the value of a initially  
    a=a-b; //now a stores the initial value of b   
    printf("\nAfter swap a=%d b=%d",a,b);    
    return 0;  
}   
